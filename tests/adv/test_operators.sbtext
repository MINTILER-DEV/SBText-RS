sprite MathTest
  when flag clicked
    say ((5) + (3))
    say ((10) - (2))
    say ((4) * (2))
    say ((8) / (2))
    say (round ((3.6)))
    say (floor ((3.6)))
    say (pick random (1) to (10))

    if <(5) > (3)> then
